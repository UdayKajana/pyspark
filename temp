| "Window Into Fixed Intervals" >> beam.WindowInto(
    beam.window.FixedWindows(120),  # 2 minutes window
    trigger=OrFinally(AfterProcessingTime.after_first(120).repeat(), AfterCount(100)), # Fire after 2 minutes or 100 records
    allowed_lateness=beam.transforms.window.Duration(seconds=300),
    accumulation_mode=AccumulationMode.ACCUMULATING
)

