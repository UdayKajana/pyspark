| "Fixed Window" >> beam.WindowInto(
      beam.window.FixedWindows(120),  # Fixed window of 2 minutes
      trigger=beam.trigger.AfterProcessingTime(120),  # Fire after 2 minutes of accumulation
      accumulation_mode=beam.trigger.AccumulationMode.DISCARDING  # Discard data after firing
)

| "Fixed Window" >> beam.WindowInto(
      beam.window.FixedWindows(120), 
      allowed_lateness=0  # Prevent late element accumulation
)
